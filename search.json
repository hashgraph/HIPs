---
layout: none
---
[
  {% for p in site.pages %}
    {% if p.url contains '404' or p.url contains '.css' or p.url contains '.json' or p.url contains 'application' or p.url contains 'core' or p.url contains 'all' or p.url contains 'mirror' or p.url contains 'informational' or p.url contains 'process' or p.url contains 'HIP-15-pseudocode' %}
    {% else %}
      {
        "title"    : "{{ p.title }}",
        "hipnum"   : "{{ p.hip }}",
        "category" : "{{ p.category }}",
        "content": {{ p.content | jsonify }},
        "url"      : "{{ site.baseurl }}{{ p.url }}"
      } {% unless forloop.last %},{% endunless %}
    {% endif %}
  {% endfor %}
]